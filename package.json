{
	"name": "file-links-helper",
	"version": "1.0.0",
	"description": "This is sample plugin for Obsidian that helps with some janitor-linking tasks.",
	"main": "src/main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"buildcss": "tailwindcss -i ./src/input.css -o ./styles.css",
		"dev-css": "tailwindcss -i ./src/input.css -o ./styles.css --watch",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"autoprefixer": "^10.4.20",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"esbuild-plugin-postcss2": "^0.1.2",
		"obsidian": "latest",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"tailwindcss": "^3.4.10",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"lucide-react": "^0.439.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	}
}
